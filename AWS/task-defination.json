{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "test-task-defination",
    "containerDefinitions": [
        {
            "name": "nginx",
            "image": "533267431526.dkr.ecr.us-east-1.amazonaws.com/ecs-nginx:$REACT_APP_VERSION",
            "portMappings": [{
                "name": "portmapping",
                "containerPort": 80,
                "hostPort": 80,
                "protocol": "tcp"
            }],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::533267431526:role/ecs_execuction_role"
}